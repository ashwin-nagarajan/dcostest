{
  "id": "marathon-demo",
  "cpus": 1,
"constraints": [["hostname", "UNIQUE"]],
    "acceptedResourceRoles": [
          "slave_public"
    ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 80, "hostPort":0}
      ]
    }
  },"uris": [
   "file:///etc/docker.tar.gz"
],
  "labels":{
    "HAPROXY_GROUP":"external"
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 10,
      "intervalSeconds": 30,
      "timeoutSeconds": 5,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    } ]
}



